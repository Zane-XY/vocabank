GET      /assets/*file                  controllers.Assets.at(path="/public", file)

GET      /                              controllers.Application.index

GET      /entries                       controllers.EntryController.entries

#GET     /entries/:id                   controllers.Application.entries

POST     /entry/delete                  controllers.EntryController.delete

POST     /entry/setRating               controllers.EntryController.setRating

POST     /entry/setSound                controllers.EntryController.setSound

POST     /entry/save                    controllers.EntryController.save

POST     /entry/submit                  controllers.EntryController.submit

POST     /entry/remoteSave              controllers.EntryController.remoteSave

#GET      /entry/remoteSave              controllers.EntryController.remoteSaveGet

GET      /lookup/:dictid/:word          controllers.DictController.lookupDef(dictid:String, word:String)

GET      /autocomplete                  controllers.DictController.autocomplete

GET      /lookupDefAsync/:word          controllers.EntryController.lookupDefAsync(word:String)

GET      /user/signUp                   controllers.UserController.signUp

POST     /user/signUp                   controllers.UserController.signUpSubmit

GET      /user/signIn                   controllers.UserController.signIn

POST     /user/signIn                   controllers.UserController.signInSubmit

